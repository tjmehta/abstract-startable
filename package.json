{
  "name": "abstract-startable",
  "version": "1.0.1",
  "description": "Abstract Class to represent entities that can be started and stopped",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build-ts && npm run _build_esm_rename && npm run _build_cjs_rename",
    "build-ts": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "_build_esm_rename": "mv dist/esm/index.js dist/esm/index.mjs && mv dist/esm/index.js.map dist/esm/index.mjs.map",
    "_build_cjs_rename": "mv dist/cjs/index.js dist/cjs/index.cjs && mv dist/cjs/index.js.map dist/cjs/index.cjs.map",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmehta/abstract-startable.git"
  },
  "keywords": [
    "abstract",
    "start",
    "stop",
    "startable",
    "stoppable",
    "base",
    "class"
  ],
  "author": "Tejesh Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmehta/abstract-startable/issues"
  },
  "homepage": "https://github.com/tjmehta/abstract-startable#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
